<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:camel="http://camel.apache.org/schema/spring" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
       http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd">


	<camelContext id="CamelTest" xmlns="http://camel.apache.org/schema/spring"
		useMDCLogging="true">
		<route id="testRoute">
			<from uri="timer://Test?fixedRate=true&amp;period=60000" />
			<log message="Log Level - INFO" />
			<log message="Log Level - DEBUG" loggingLevel="DEBUG" logName="com.test.sample" />
		</route>
	</camelContext>


</beans>
