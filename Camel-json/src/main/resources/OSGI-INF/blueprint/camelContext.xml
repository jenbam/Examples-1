            <from id="_from1" uri="timer:Test?period=6000"/>
            <log id="_log1" message="Expiring message: ${body}"/>
            <setHeader headerName="MSAZ_DOCDB_DEL_DOC_ID" id="_setHeader1">
                <jsonpath>$.id</jsonpath>
            </setHeader>
            <log id="_log2" message="Expiring message id: ${in.headers.MSAZ_DOCDB_DEL_DOC_ID}"/>
            <setHeader headerName="MSAZ_BLOBSTORE_PATH" id="_setHeader2">
                <jsonpath>$.messageDocumentLocation1</jsonpath>
            </setHeader>
            <log id="_log3" message="Expiring BlobStore path: ${in.headers.MSAZ_BLOBSTORE_PATH}"/>
        </route>
    </camelContext>
</blueprint>
